number = [1, 2, 3, 0, 4, 6, 0, 0, 9]  # 3, 6, 7


def valid(number):
    zero = find_zero(number)
    if not zero:
        print(number)
        return True
    else:
        position = zero
    for i in range(1, 10):
        if swap(number, position):
            number[position] = i
            if valid(number):
                return True
            number[position] = 0
    return False


def find_zero(number): 
    for i in range(len(number)):
        if number[i] == 0:
            return i
    return None


def swap(number, n): # TODO
    for i in range(len(number)):
        if number[i] != n and number[i] != i:
            return True


valid(number)
